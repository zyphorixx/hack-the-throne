# Copy this file to .env and fill in your API keys
# These are required for the application to work

# Hugging Face token for pyannote models
PYANNOTE_AUTH_TOKEN=your_huggingface_token_here

# Groq API key for LLM inference
GROQ_API_KEY=your_groq_api_key_here

# Convex cloud URL for database
CONVEX_URL=https://your-project.convex.cloud

# ElevenLabs API key for text-to-speech
ELEVENLABS_API_KEY=your_elevenlabs_key_here

# Sarvam API key
SARVAM_API_KEY=your_sarvam_key_here

# MongoDB URI (optional, for inference service)
MONGODB_URI=
